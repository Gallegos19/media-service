{
  "name": "media-service",
  "version": "1.0.0",
  "description": "Media microservice for handling file uploads and management",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "_moduleAliases": {
    "@application": "./src/application",
    "@domain": "./src/domain",
    "@infrastructure": "./src/infrastructure",
    "@shared": "./src/shared",
    "@interfaces": "./src/interfaces",
    "@config": "./src/config"
  },
  "scripts": {
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^5.0.0",
    "@types/socket.io": "^3.0.2",
    "aws-sdk": "^2.1488.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-async-handler": "^1.2.0",
    "express-fileupload": "^1.4.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.2.0",
    "inversify": "^7.5.4",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "socket.io": "^4.8.1",
    "source-map-support": "^0.5.21",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "module-alias": "^2.2.3",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.23",
    "@types/express-fileupload": "^1.4.3",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^24.0.10",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.0",
    "prettier": "^3.1.1",
    "prisma": "^5.6.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
